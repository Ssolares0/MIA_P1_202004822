mkdisk -size=5000 -unit=K -fit=BF


#Crea una partición primaria llamada Particion1 de 300 kb
#con el peor ajuste en el disco A.dsk

fdisk -size=1000 -driveletter=A -name=Particion1
#Crea una partición extendida dentro del disco A.dsk de 300 kb
#Tiene el peor ajuste

fdisk -type=E -driveletter=A -unit=K -name=Particion2 -size=600
#Crea una partición lógica con el mejor ajuste, llamada Partición 3,
#de 1 Mb en el disco A

fdisk -size=300 -type=L -unit=K -fit=bf -driveletter=A -name=Particion3

fdisk -size=300 -type=L -unit=K -fit=bf -driveletter=A -name=Particion4

mkdisk -size=3000 -unit=K -fit=BF

fdisk -size=1000 -driveletter=B -name=Particion1

mount -driveletter=A -name=Particion1 #id=A122
224unmount -id=A122

mkfs -type=full -id=A122
